{
  "name": "metaline/wp-api-client",
  "description": "A PHP client for the WordPress REST API, based on Guzzle.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniele De Nobili",
      "email": "daniele@metaline.it",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.4",
    "psr/log": "^1.0 || ^2.0 || ^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "jeroen/psr-log-test-doubles": "^2.2 || ^3.2"
  },
  "autoload": {
    "psr-4": {
      "MetaLine\\WordPressAPIClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MetaLine\\WordPressAPIClient\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "php vendor/bin/phpunit"
    ]
  }
}
